<module xmlns="urn:jboss:module:1.9" name="org.jboss.invocation">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${org.jboss.invocation:jboss-invocation}"/>
    </resources>

    <dependencies>
        <module name="jakarta.interceptor.api"/>
        <module name="jakarta.transaction.api" optional="true"/>
        <module name="org.jboss.classfilewriter"/>
        <module name="org.jboss.logging"/>
        <module name="org.wildfly.common"/>
        <module name="org.wildfly.security.elytron-private"/>
    </dependencies>
</module>
