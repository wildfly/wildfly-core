<?xml version='1.0' encoding='UTF-8'?>
<host xmlns="urn:jboss:domain:13.0">
    <management>
        <security-realms>
            <!-- Set of realms using file based stores. -->
        
            <security-realm name="IdentityOnly">
                <server-identities>
                    <ssl>
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>            
            </security-realm>
            <security-realm name="AliasedIdentityOnly">
                <server-identities>
                    <ssl>
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir" keystore-password="keystore_password" key-password="key_password" alias="TheAlias" />
                   </ssl>
                </server-identities>            
            </security-realm>            
            <security-realm name="IdentityOtherAuthentication">
                <server-identities>
                    <ssl>
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>
                <authentication>
                    <local default-user="$local" skip-group-loading="true"/>
                    <properties path="mgmt-users.properties" relative-to="jboss.server.config.dir"/>
                </authentication>                            
            </security-realm>
            <security-realm name="IdentityOtherAndTrustAuthentication">
                <server-identities>
                    <ssl>
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>
                <authentication>                                    
                    <truststore path="server.truststore" relative-to="jboss.server.config.dir" keystore-password="truststore_password" />
                    <local default-user="$local" skip-group-loading="true"/>
                    <properties path="mgmt-users.properties" relative-to="jboss.server.config.dir"/>
                </authentication>                            
            </security-realm>
            <security-realm name="IdentityWithTruststoreCredentialReference">
                <server-identities>
                    <ssl>
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir" keystore-password="keystore_password" />
                    </ssl>
                </server-identities>
                <authentication>
                    <truststore path="server.truststore" relative-to="jboss.server.config.dir">
                        <keystore-password-credential-reference clear-text="keystore_password"/>
                    </truststore>
                    <local default-user="$local" skip-group-loading="true"/>
                    <properties path="mgmt-users.properties" relative-to="jboss.server.config.dir"/>
                </authentication>
            </security-realm>
            <security-realm name="IdentityWithSslCredentialReference">
                <server-identities>
                    <ssl>
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir">
                            <keystore-password-credential-reference clear-text="keystore_password"/>
                        </keystore>
                    </ssl>
                </server-identities>
                <authentication>
                    <truststore path="server.truststore" relative-to="jboss.server.config.dir" keystore-password="truststore_password" />
                    <local default-user="$local" skip-group-loading="true"/>
                    <properties path="mgmt-users.properties" relative-to="jboss.server.config.dir"/>
                </authentication>
            </security-realm>

            <!-- Protocol and Cipher Suite Selection -->
            <security-realm name="ProtocolSuite">
                <server-identities>
                    <ssl protocol="TLSv1.1">
                        <engine enabled-cipher-suites="TLS_PSK_WITH_NULL_SHA256 TLS_PSK_WITH_NULL_SHA384 TLS_PSK_WITH_RC4_128_SHA" enabled-protocols="TLSv1 TLSv1.1" />
                        <keystore path="server.keystore" relative-to="jboss.server.config.dir" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>
            </security-realm>

            <!-- Set of realms using security provider based stores. -->
        
            <security-realm name="IdentityOnlyProvider">
                <server-identities>
                    <ssl>
                        <keystore provider="PKCS11-ServerKeyStore" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>            
            </security-realm>            
            <security-realm name="IdentityOtherAuthenticationProvider">
                <server-identities>
                    <ssl>
                        <keystore provider="PKCS11-ServerKeyStore" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>
                <authentication>
                    <local default-user="$local" skip-group-loading="true"/>
                    <properties path="mgmt-users.properties" relative-to="jboss.server.config.dir"/>
                </authentication>                            
            </security-realm>
            <security-realm name="IdentityOtherAndTrustAuthenticationProvider">
                <server-identities>
                    <ssl>
                        <keystore provider="PKCS11-ServerKeyStore" keystore-password="keystore_password" />
                   </ssl>
                </server-identities>
                <authentication>                                    
                    <truststore provider="PKCS11-ServerTrustStore" keystore-password="truststore_password" />
                    <local default-user="$local" skip-group-loading="true"/>                    
                    <properties path="mgmt-users.properties" relative-to="jboss.server.config.dir"/>
                </authentication>                            
            </security-realm>            
        </security-realms>

        <management-interfaces>
            <native-interface>
                <socket interface="management" port="${jboss.management.native.port:9999}" />
            </native-interface>
        </management-interfaces>
    </management>

    <interfaces>
        <interface name="management">
            <inet-address value="${jboss.bind.address.management:127.0.0.1}" />
        </interface>
    </interfaces>

</host>
